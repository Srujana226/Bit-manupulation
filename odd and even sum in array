#include <stdio.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }  
    int oddsum=0, evensum=0;
    for(int i=0; i<n; i++)
    {
        if(arr[i]%2==0)
        {
            evensum+=arr[i];
        }
        else
        {
            oddsum+=arr[i];
        }
    }
    printf("%d %d", oddsum, evensum);
    return 0;
}
