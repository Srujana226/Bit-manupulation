#include <stdio.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    } 
    int c1=0, c2=0, c3=0;
    for(int i=0; i<n; i++)
    {
        if(arr[i]==0)
        {
            c1+=1;
        }
        else if(arr[i]>0)
        {
            c2+=1;
        }
        else
        {
            c3+=1;
        }
    }
    printf("%d %d %d",c1, c2, c3);
    return 0;
}
